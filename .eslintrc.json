{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "object-curly-newline": "off",
    "comma-dangle": "off",
    "consistent-return": "off",
    "class-methods-use-this": "off"
  }
}
